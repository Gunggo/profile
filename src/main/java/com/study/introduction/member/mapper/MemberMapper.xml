<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.introduction.member.mapper.MemberMapper">

    <select id="selectMemberList"  resultType="com.study.introduction.member.bean.MemberBean">
        SELECT
            *
        FROM member
    </select>

    <select id="login" resultType="com.study.introduction.member.bean.MemberBean">
        SELECT
            *
        FROM member
        WHERE mb_email = #{mbEmail}
        AND mb_pw = #{mbPw}
    </select>
</mapper>